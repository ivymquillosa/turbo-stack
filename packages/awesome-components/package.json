    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.15.3",
    "@tanstack/react-virtual": "^3.2.0",
    "class-variance-authority": "^0.7.0",
    "react-aria-components": "^1.1.1",
    "react-icons": "^5.0.1"
